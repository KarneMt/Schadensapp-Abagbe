@model Schadensapp.Models.BearbeitungsstellenView

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Bearbeitungsstelle</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            MAP-Gruppe
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bearbeitungsstelle.MapGruppe)
        </dd>
        <dt class = "col-sm-2">
            Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bearbeitungsstelle.Name)
        </dd>
        <dt class = "col-sm-2">
            Telefonnummer
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bearbeitungsstelle.Telefone)
        </dd>
        <dt class = "col-sm-2">
            E-Mail
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Bearbeitungsstelle.EMail)
        </dd>
        <dt class = "col-sm-2">
            Aktiviert
        </dt>
        <dd class = "col-sm-10">
            @if (Model.Bearbeitungsstelle.IsActive == true)
            {
                <p>Ja</p>
            }
            else
            {
                <p>Nein</p>
            }
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index">Zurück zur Liste</a>
</div>
